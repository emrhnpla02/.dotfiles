{
  "workbench.iconTheme": "material-icon-theme",
  "workbench.startupEditor": "none",
  "gitlens.advanced.messages": {
    "suppressGitMissingWarning": true
  },
  "editor.cursorBlinking": "expand",
  "[jsonc]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[vue]": {
    "editor.defaultFormatter": "johnsoncodehk.volar"
  },
  "editor.fontFamily": "'Fira Code'",
  "editor.fontLigatures": true,
  "editor.fontWeight": 355,
  "editor.bracketPairColorization.enabled": true,
  "editor.guides.bracketPairs": "active",
  "[html]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "editor.lineHeight": 21,
  "editor.lineNumbers": "relative",
  "editor.smoothScrolling": true,
  "editor.cursorSmoothCaretAnimation": true,
  "explorer.confirmDelete": false,
  "[javascript]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "security.workspace.trust.untrustedFiles": "open",
  "editor.suggestSelection": "first",
  "vsintellicode.modify.editor.suggestSelection": "automaticallyOverrodeDefaultValue",
  "C_Cpp.updateChannel": "Insiders",
  "terminal.integrated.defaultProfile.linux": "zsh",
  "editor.linkedEditing": true,
  "[json]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "explorer.confirmDragAndDrop": false,
  "workbench.colorTheme": "Nord",
  "workbench.colorCustomizations": {
    "jumpy2.labelFontColor": "#FFFFFF",
    "jumpy2.labelBackgroundColor": "#44748F"
  },
  "editor.formatOnPaste": true,
  "editor.formatOnSave": true,
  "editor.formatOnType": true,
  "[css]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "workbench.enableExperiments": false,
  "workbench.settings.enableNaturalLanguageSearch": false,
  "update.showReleaseNotes": false,
  "extensions.autoCheckUpdates": false,
  "editor.minimap.enabled": false,
  "liveServer.settings.donotShowInfoMsg": true,
  "vscode-neovim.neovimExecutablePaths.linux": "/usr/bin/nvim",
  "files.exclude": {
    "**/.git": false,
    "**/bin": false,
    "**/obj": false,
    "**/Properties": false,
    "**/.hg": false,
    "**/CVS": false,
    "**/.DS_Store": false,
    "**/Thumbs.db": false,
    "**/.vscode": false
  },
  "cSpell.userWords": [
    "appsettings",
    "archlinux",
    "aspnetcore",
    "Blazor",
    "Bulma",
    "dotfiles",
    "Emirhan",
    "emrhnpla",
    "epfetch",
    "figcaption",
    "Fira",
    "firacode",
    "Hsts",
    "Lemmy",
    "monospace",
    "navmenu",
    "neofetch",
    "nord",
    "nowrap",
    "onclick",
    "pacman",
    "Pala",
    "Pepe",
    "sadpepe",
    "tailwindcss",
    "udemy",
    "vuejs",
    "wwwroot"
  ],
  "editor.defaultFormatter": "esbenp.prettier-vscode",
  "[aspnetcorerazor]": {
    "editor.defaultFormatter": "ms-dotnettools.csharp"
  },
  "[svg]": {
    "editor.defaultFormatter": "jock.svg"
  },
  "[csharp]": {
    "editor.defaultFormatter": "ms-dotnettools.csharp"
  },
  "vssolution.solutionExplorerIcons": "current-theme",
  "vim.leader": " ",
  "vim.useSystemClipboard": true,
  "vim.hlsearch": true,
  "vim.enableNeovim": true,
  "vim.neovimPath": "/usr/bin/nvim",
  "vim.visualstar": true,
  "vim.easymotion": true,
  "vim.easymotionMarkerBackgroundColor": "#44748F",
  "vim.easymotionMarkerForegroundColorOneChar": "#FFFFFF",
  "vim.easymotionMarkerForegroundColorTwoCharFirst": "#44748F",
  "vim.easymotionMarkerForegroundColorTwoCharSecond": "#FFFFFF",
  "vim.normalModeKeyBindingsNonRecursive": [
    {
      "before": ["f"],
      "commands": ["jumpy2.toggle"]
    }
  ],
  "svg.preview.mode": "img",
  "window.zoomLevel": -1,
  "cSpell.enableFiletypes": ["aspnetcorerazor"],
  "editor.multiCursorModifier": "ctrlCmd"
}
